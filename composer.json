{
    "name": "kesty/laravel-notifier",
    "description": "Simple notification widget for Laravel ^7.0",
    "keywords": ["notification","notify","laravel-package","laravel-notification package","alert"],
    "readme": "README.md",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Olubunmi Tosin",
            "email": "olubunmivictor6@gmail.com",
            "homepage": "http://teqpace.com/victor",
            "role" : "Developer"
        }
    ],
    "support": {
        "wiki": "https://github.com/olubunmitosin/laravel-notifier/wiki",
        "issues": "https://github.com/olubunmitosin/laravel-notifier/issues"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Kesty\\LaravelNotifier\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kesty\\LaravelNotifier\\NotifierServiceProvider"
            ],
            "aliases": {
                "KestyNotify": "Kesty\\LaravelNotifier\\Facade\\Notifier"
            }
        }
    }
}
